<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:ScrollPanel>
		<g:VerticalPanel styleName="fullbox">
			<g:HTMLPanel styleName="header1">
				<div>DATA BPRS</div>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="headerspacer">
				<div></div>
			</g:HTMLPanel>
			<g:HorizontalPanel styleName="widgetbox">
				<g:HTMLPanel styleName="fullbox">
					<table class="fullbox">
						<tr>
							<td class="halfbox">
								<div class="compositebox">
									<div class="fieldtitle">Nama</div>
									<g:TextBox styleName="fieldtextbox" ui:field="name" />
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Alamat</div>
									<g:TextBox styleName="fieldtextbox" ui:field="address" />
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Kota, kode pos</div>
									<div>
										<g:TextBox styleName="fieldtextbox" ui:field="city" />
										<g:TextBox styleName="fieldtextbox" ui:field="zipCode" />
									</div>
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Propinsi</div>
									<g:TextBox styleName="fieldtextbox" ui:field="province" />
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Nomer telpon</div>
									<g:TextBox styleName="fieldtextbox" ui:field="phone" />
								</div>
							</td>
						</tr>
					</table>
				</g:HTMLPanel>
			</g:HorizontalPanel>
			<g:HTMLPanel styleName="widgetboxspacer">
				<div></div>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="header1">
				<div>DATA LAPORAN BI</div>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="headerspacer">
				<div></div>
			</g:HTMLPanel>
			<g:HorizontalPanel styleName="widgetbox">
				<g:HTMLPanel styleName="fullbox">
					<table class="fullbox">
						<tr>
							<td class="halfbox">
								<div class="compositebox">
									<div class="fieldtitle">Kode BI</div>
									<g:TextBox styleName="fieldtextbox" ui:field="biCode" />
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Direktur</div>
									<g:TextBox styleName="fieldtextbox" ui:field="director" />
								</div>
								<div class="compositebox">
									<div class="fieldtitle">Penanggung jawab laporan</div>
									<g:TextBox styleName="fieldtextbox" ui:field="pic" />
								</div>
							</td>
						</tr>
					</table>
				</g:HTMLPanel>
			</g:HorizontalPanel>
			<g:HTMLPanel styleName="widgetboxspacer">
				<div></div>
			</g:HTMLPanel>
			<g:HorizontalPanel>
				<g:Button styleName="fieldbigbutton" ui:field="btnSave">Simpan</g:Button>
				<g:HTMLPanel styleName="widgetboxspacer">
					<div class="fieldspacer"></div>
				</g:HTMLPanel>
				<g:Button styleName="fieldbigbutton" ui:field="btnCancel">Batal</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 